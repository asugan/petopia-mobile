{
  "expo": {
    "name": "Petopia",
    "slug": "petopia-petcare",
    "version": "1.1.0",
    "locales": {
      "en-US": "./locales/native/en-US.json",
      "en-GB": "./locales/native/en-GB.json",
      "tr": "./locales/native/tr.json",
      "de-DE": "./locales/native/de-DE.json",
      "fr-FR": "./locales/native/fr-FR.json",
      "es-ES": "./locales/native/es-ES.json",
      "it": "./locales/native/it.json",
      "pt-BR": "./locales/native/pt-BR.json",
      "pt-PT": "./locales/native/pt-PT.json",
      "ru": "./locales/native/ru.json",
      "ja": "./locales/native/ja.json",
      "ko": "./locales/native/ko.json",
      "ar-SA": "./locales/native/ar-SA.json",
      "zh-Hans": "./locales/native/zh-Hans.json",
      "zh-Hant": "./locales/native/zh-Hant.json",
      "sv": "./locales/native/sv.json",
      "th": "./locales/native/th.json",
      "uk-UA": "./locales/native/uk-UA.json",
      "vi": "./locales/native/vi.json",
      "sk": "./locales/native/sk.json",
      "ro": "./locales/native/ro.json",
      "pl": "./locales/native/pl.json",
      "nb": "./locales/native/nb.json",
      "nl-NL": "./locales/native/nl-NL.json",
      "ms": "./locales/native/ms.json",
      "hu": "./locales/native/hu.json",
      "hi": "./locales/native/hi.json",
      "hr": "./locales/native/hr.json",
      "he": "./locales/native/he.json",
      "el": "./locales/native/el.json",
      "fi": "./locales/native/fi.json",
      "da": "./locales/native/da.json",
      "cs": "./locales/native/cs.json",
      "ca": "./locales/native/ca.json"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "petopia-petcare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petopia.petcare",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleLocalizations": [
          "en-US",
          "en-GB",
          "tr",
          "de-DE",
          "fr-FR",
          "es-ES",
          "it",
          "pt-BR",
          "pt-PT",
          "ru",
          "ja",
          "ko",
          "ar-SA",
          "zh-Hans",
          "zh-Hant",
          "sv",
          "th",
          "uk-UA",
          "vi",
          "sk",
          "ro",
          "pl",
          "nb",
          "nl-NL",
          "ms",
          "hu",
          "hi",
          "hr",
          "he",
          "el",
          "fi",
          "da",
          "cs",
          "ca"
        ]
      }
    },
    "android": {
      "package": "com.petopia.petcare",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground.png",
        "backgroundImage": "./assets/images/background.png",
        "monochromeImage": "./assets/images/foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "petopia-petcare"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": []
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#D3DFF1"
        }
      ],
      "expo-image-picker",
      "expo-secure-store",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#007AFF",
                "dark": "#0A84FF"
              },
              "textColorPrimary": {
                "light": "#007AFF",
                "dark": "#0A84FF"
              }
            },
            "timePicker": {
              "background": {
                "light": "#FFFFFF",
                "dark": "#1C1C1E"
              },
              "numbersBackgroundColor": {
                "light": "#007AFF",
                "dark": "#0A84FF"
              },
              "numbersTextColor": {
                "light": "#FFFFFF",
                "dark": "#FFFFFF"
              }
            }
          }
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b829ebb6-cac8-4731-94f5-4b130b65a827"
      }
    },
    "owner": "asugan"
  }
}
